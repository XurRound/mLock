<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="me.xurround.mlock.controller.MainController"
            prefWidth="800.0"
            prefHeight="500.0"
            stylesheets="@../style/main.css"
